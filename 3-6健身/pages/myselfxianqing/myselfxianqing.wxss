/* pages/myselfxianqing/myselfxianqing.wxss */
page{
  width: 100%;
  height: 100%;
}
.container{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  text-align: center;
}
.img{
  width: 100%;
  position: relative;
}
.img image{
  width: 100%;
}
.neicon{
  width: 100%;
  text-align: center;
  position: absolute;
  top:50%;
  color: #fff;
  transform: translate(0,-50%);
  font-size: 25px;
}
.nai{
  font-weight: 600;
  font-size: 20px;
}
.feng{
  background: #000;
  font-size: 12px;
  width: 15%;
  padding: 8rpx 0;
  margin: 0 auto;
  background: #fff;
  color: #000;
  margin-top:50rpx;
}
.wangcheng{
  width: 100%;
  border-bottom: 1rpx solid #ccc;
  box-sizing: border-box;
  padding-bottom: 50rpx;
  margin:  0 0 50rpx 0;

}
.tet{
  font-weight: 600;
  margin: 50rpx 0;

}
.tet text{
  font-size: 12px;
  font-weight: 500;
}
.btna{
  width: 80%;
  border: 1rpx solid #000;
  border-radius: 10rpx;
  margin: 40rpx auto;
  font-size: 12px;
  padding: 16rpx 0;
  box-sizing: border-box;
}
.btnb{
  width: 60%;
  background: #000;
  color: #fff;
  font-size: 16px;
}
.zhezhao{
  width: 100%;
  height: 100%;
  position: fixed;
  top:0;
  background: rgba(000,000,000,0.3);
}
.neibox{
  width: 80%;
  position: absolute;
  background: #fff;
  top:50%;
  left:50%;
  transform: translate(-50%,-50%);
  padding: 30rpx;
  box-sizing: border-box;
}
.txt{
  font-size: 14px;
}
.btnc{
  width: 80%;
  font-size: 13px;
  background: #000;
  margin: 20rpx auto;
  color: #fff; 
}