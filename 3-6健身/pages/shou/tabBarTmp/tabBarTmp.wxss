.tabcon{
  width: 100%;
  position: fixed;
  bottom: 0;
  border-top:1rpx solid #aaa;
  display: flex;
  box-sizing: border-box;
   background-color: #fff;
}
.tan{
  width: 33.5%;
}
.tan>view{
  width: 60rpx;
  margin: 0 auto;
  font-size: 12px;
  color: #bfbfbf;
  text-align: center;
  padding: 10rpx 0 4rpx 0;
}
.tan>view image{
  width: 90%;
  height: 50rpx;
}